{
  "name": "komodo-mcp-server",
  "version": "1.0.0",
  "description": "MCP Server for Komodo API integration with Trae IDE",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server-consolidated.js",
    "start:modular": "node dist/server-new.js",
    "dev": "tsx src/server-consolidated.ts",
    "dev:modular": "tsx src/server-new.ts",
    "dev:old": "tsx src/server.ts",
    "build:xmcp": "xmcp build",
    "start:xmcp": "node dist/http.js",
    "dev:xmcp": "xmcp dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "mcp",
    "komodo",
    "trae",
    "ide"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.12.1",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.1",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "komodo_client": "latest",
    "xmcp": "^0.3.4",
    "zod": "3"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  }
}
